				<string>echo '### Workflow version'
/usr/libexec/PlistBuddy -c 'print version' info.plist

echo
echo '### Alfred version'
/usr/bin/osascript -e 'tell application id "com.runningwithcrayons.Alfred" to return version'

echo
echo '### macOS version'
/usr/bin/sw_vers -productVersion

echo
echo '### Architecture'
/usr/bin/arch
echo

echo
echo '### Workflow environment variables'
/usr/libexec/PlistBuddy -c 'print variables' info.plist

echo
echo '### Full Disk Access'

if sqlite3 '/Library/Application Support/com.apple.TCC/TCC.db' '.quit' 2&gt; /dev/null; then
  echo 'Granted'
else
  echo 'NOT granted'
fi

echo
echo '### Google drive paths'

while IFS= read -r gdrive_path; do
  absolute_path="${gdrive_path/#\~/${HOME}}"

  if [[ -d "${absolute_path}" ]]; then
    echo "Exists: ${gdrive_path}"
    find "${absolute_path}" | wc -l | tr -d ' \n'
    echo ' paths'
  else
    echo "Does NOT Exist: ${gdrive_path}"
  fi
done &lt; &lt;(sed 's/,/\n/g' &lt;&lt;&lt; "${google_drive_path}" | sed 's/ *//' | sed 's/ *$//')

echo
echo '### Cache dir'

readonly cache_dir="${HOME}/Library/Caches/com.runningwithcrayons.Alfred/Workflow Data/com.alfredapp.googledrive"
readonly cache_files="$(ls -1 "${cache_dir}")"

if [[ -n "${cache_files}" ]]; then
  echo "${cache_files}"
else
  echo 'Files NOT present'
fi

echo
echo '### Cache file'

if [[ -f "${cache_dir}/cache.db" ]]; then
  stat "${cache_dir}/cache.db"
else
  echo 'Does NOT exist'
fi

echo
echo '### Temporary cache'

if [[ -f "${cache_dir}/tmp.db" ]]; then
  stat "${cache_dir}/tmp.db"
else
  echo 'Does NOT exist'
fi

echo
echo '### Build progress'

if ps A | grep --quiet '[r]uby.*Alfred.*rebuild_cache'; then
  echo 'Running'
else
  echo 'NOT running'
fi

echo
echo '### Launchd job'

if launchctl list | grep -q 'com.alfredapp.launchd.googledrivecache'; then
  echo -n 'Loaded: '
  launchctl list | grep 'com.alfredapp.launchd.googledrivecache'
else
  echo 'NOT loaded'
fi

if [[ -f "${HOME}/Library/LaunchAgents/com.alfredapp.launchd.googledrivecache.plist" ]]; then
  echo 'Installed'
else
  echo 'NOT installed'
fi</string>
